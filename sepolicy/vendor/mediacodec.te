# /system/lib/omx/
allow mediacodec system_file:dir r_dir_perms;

# /sys/class/video4linux/video{6,7}/name
allow mediacodec sysfs_mfc:file r_file_perms;
allow mediacodec sysfs_mfc:lnk_file read;

allow mediacodec sysfs_mfc:dir { open read search };

# sysfs_virtual
allow mediacodec sysfs_virtual:dir { open read search };
allow mediacodec sysfs_virtual:file { open read };

binder_call(mediacodec, hal_power_default)
