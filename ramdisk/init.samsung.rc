    restorecon /sys/kernel/debug/tracing/trace_marker

    chown system radio /efs
    chmod 0771 /efs
    restorecon /efs

    chown system system /spu
    chmod 0771 /spu
    restorecon /spu

    chown system system /dqmdbg
    chmod 0770 /dqmdbg
