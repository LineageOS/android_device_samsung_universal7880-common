		<ctl name="ADCL Mixer MIC1L Switch" value="0" />
		<ctl name="ADCL Mixer MIC2L Switch" value="1" />
		<ctl name="ADCL Mixer MIC3L Switch" value="0" />
		<ctl name="ADCL Mixer LINELL Switch" value="0" />
