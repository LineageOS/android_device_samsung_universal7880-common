		<ctl name="DMIC2 DMIC2 On" value="0" />
		<ctl name="MIC2 MIC2 On" value="1" />
		<ctl name="MIC3 MIC3 On" value="0" />
		<ctl name="LINEIN LINEIN On" value="0" />
	</path>

	<!-- L ch: 2nd-mic / R ch: main-mic -->
	<path name="dev-dual-invert-mic">
